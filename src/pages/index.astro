---
import Layout from '../layouts/Layout.astro';
import HomeHero from '../components/HomeHero.astro';
import Reviews from '../components/Reviews.astro';
import FollowUs from '../components/FollowUs.astro'
---
 
<Layout title="Welcome to Astro." isHome={true}>
	<HomeHero textdark={false}/>
  <FollowUs />
  <Reviews />
</Layout>

<link
  rel="stylesheet"
  href="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.css"
/>
<link rel="stylesheet" href="/assets/style/slider.css"/>
<script
  is:inline
  src="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.js"
></script>
<script is:inline src="/assets/js/main.js"></script>

